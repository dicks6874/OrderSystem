<div class="classify">
    <ul class="menu-catagory nav nav-tabs">
        <% if (menuCatagatory) { %>
        <% for ( var i = 0 ; i < menuCatagatory.length ; i++){ %>
        <li class="nav-item <%= i === 0 ? 'active' : '' %>">
            <a class="nav-link <%= i === 0 ? 'active' : '' %>"
                data-target='#menu-<%= menuCatagatory[i].MenuCatagatoryId %>' data-toggle="tab">
                <span><%= menuCatagatory[i].Name %>
                </span>
            </a>
        </li>
        <% } %>
        <% } %>
    </ul>
    <div class="tab-content classify-wrap">
        <% if (menuCatagatory) { %>
        <% for ( var i = 0 ; i < menuCatagatory.length ; i++){ %>
        <div class="tab-pane fade" id="menu-<%= menuCatagatory[i].MenuCatagatoryId %>" role="tabpanel"
            aria-labelledby="menu-<%= menuCatagatory[i].MenuCatagatoryId %>-tab">
            <div class="row menu-header"><%= menuCatagatory[i].Name %>
            </div>
            <% if (menuItem) { %>
            <% var items = menuItem.filter(function(value){ return value.MenuCatagatoryId==menuCatagatory[i].MenuCatagatoryId;}); %>
            <% items.forEach(function(item){ %>
            <div class="row menu-row">
                <table style="width: calc(100vw - 4rem);">
                    <tr>
                        <td style="width: 2rem;">
                            <img class="menu-img" src="<%= item.Image %>" alt="sushi1" width="100" height="100">
                        </td>
                        <td>
                            <div style="height:4rem">
                                <div class="menu-name row menu-item-row"><%= item.Name %>
                                </div>
                                <div class="menu-price row menu-item-row">
                                    <div style="width: 100%;">
                                        $<%= item.Price.toFixed(1) %>
                                        <button class="btn float-right custom-button"><i
                                                menuItemId="<%= item.MenuItemId %>" name="AddButton"
                                                class="fa fa-plus-circle fa-lg" aria-hidden="true"></i></button>
                                    </div>
                                </div>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
            <% }); %>
            <% } %>
        </div>
        <% } %>
        <% } %>
    </div>
</div>